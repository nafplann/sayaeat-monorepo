{
    "name": "sayaeat/shared",
    "description": "Shared package for SayaEat services (Models, Enums, Utils, DTOs)",
    "type": "library",
    "license": "proprietary",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0",
        "illuminate/http": "^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "pestphp/pest": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "SayaEat\\Shared\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SayaEat\\Shared\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

